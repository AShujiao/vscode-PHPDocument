{
    "name": "phpdocument",
    "displayName": "PHPDocument",
    "description": "View the php function manual directly in vscode (在vscode 中查看 php 参考手册)",
    "version": "1.0.5",
    "publisher": "manasxx",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "php",
        "document",
        "help",
        "doc"
    ],
    "one_title": true,
    "activationEvents": [],
    
    "icon": "resources/php.png",
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "PHPDocument.Readline",
                "title": "php Document : Readline"
            },
            {
                "command": "PHPDocument.gotoDocument",
                "title": "PHP Document"
            }
        ],
        "configuration": {
            "title": "PHP Document language config",
            "properties": {
                "PHPDocument.language": {
                    "type": "string",
                    "enum": [
                        "English",
                        "Brazilian",
                        "Chinese",
                        "French",
                        "German",
                        "Japanese",
                        "Romanian",
                        "Russian",
                        "Spanish",
                        "Turkish"
                    ],
                    "default": "English",
                    "description": "document language"
                }
            }
        },
        "keybindings": [
            {
                "command": "PHPDocument.gotoDocument",
                "key": "ctrl+alt+p",
                "mac": "cmd+alt+p",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "PHPDocument.gotoDocument",
                    "group": "navigation@1",
                    "when": "resourceLangId == php"
                }
            ]
        }
    },
    "bugs": {
        "url": "https://github.com/AShujiao/vscode-phpDocument/issues",
        "email": "a916817706@live.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AShujiao/vscode-phpDocument.git"
    },
    "homepage": "https://github.com/AShujiao/vscode-phpDocument/blob/master/README.md",
    "scripts": {
        "vscode:prepublish": "",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.6",
        "typescript": "^2.6.1",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "@types/request": "^2.48.1",
        "request": "^2.88.0",
        "url": "^0.11.0"
    }
}
