{
    "name": "phpDocumen",
    "displayName": "phpDocumen",
    "description": "phpDocumen",
    "version": "0.0.1",
    "publisher": "manasxx",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "php",
        "document",
        "help",
        "doc"
    ],
    "activationEvents": [
        "onCommand:phpDocumen.readline",
        "onCommand:phpDocumen.gotoDocument"
    ],
    "icon": "resources/php.png",
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "phpDocumen.readline",
                "title": "php Documen : Readline"
            },
            {
                "command": "phpDocumen.gotoDocument",
                "title": "php Document"
            }
        ],
        "configuration": {
            "title": "php Document language config",
            "properties": {
                "phpDocumen.language": {
                    "type": "string",
                    "enum": [
                        "English",
                        "Brazilian",
                        "Chinese",
                        "French",
                        "German",
                        "Japanese",
                        "Romanian",
                        "Russian",
                        "Spanish",
                        "Turkish"
                    ],
                    "default": "English",
                    "description": "document language"
                }
            }
        },
        "keybindings": [
            {
                "command": "phpDocumen.gotoDocument",
                "key": "ctrl+alt+p",
                "mac": "cmd+alt+p",
                "when": "editorTextFocus"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "phpDocumen.gotoDocument",
                    "group": "navigation@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@types/request": "^2.47.1",
        "request": "^2.88.0"
    }
}
